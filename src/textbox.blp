using Gtk 4.0;
using Adw 1;

template $TextBox : Adw.Bin {
      TextView textview {
        styles ["textbox"]
        hexpand: true;
        bottom-margin: 12;
        left-margin: 12;
        right-margin: 12;
        top-margin: 12;
        editable: true;
        cursor-visible: true;
        wrap-mode: char;
      }
}